    "@types/jest": "latest",
    "@types/node": "latest",
    "eslint": "latest",
    "jest": "latest",
    "nuxt": "latest",
    "siroc": "latest",
    "standard-version": "latest"
  },
  "peerDependencies": {
    "eslint": ">=7"

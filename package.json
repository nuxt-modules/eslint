{
  "name": "@nuxtjs/eslint-module",
  "version": "3.0.2",
  "description": "ESLint module for Nuxt.js",
  "repository": "nuxt-community/eslint-module",
  "license": "MIT",
  "contributors": [
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>"
